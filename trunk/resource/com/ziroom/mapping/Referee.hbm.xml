<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ziroom.entity.Referee" table="t_referee"
		catalog="ziroom">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native"></generator>
		</id>
		<property name="sysCustId" type="java.lang.Integer">
			<column name="sys_cust_id" />
		</property>
		<property name="sysContractId" type="java.lang.Integer">
			<column name="sys_contract_id" />
		</property>
		<property name="custName" type="java.lang.String">
			<column name="cust_name" length="50" />
		</property>
		<property name="telphone" type="java.lang.String">
			<column name="telphone" length="50" />
		</property>
		<property name="certNo" type="java.lang.String">
			<column name="cert_no" length="50" />
		</property>
	</class>
</hibernate-mapping>
